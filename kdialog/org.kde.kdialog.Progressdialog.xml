<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.kdialog.Progressdialog">
    <method name="setMaximum">
      <arg name="maximum" type="i" direction="in"/>
    </method>
    <method name="maximum">
      <arg type="i" direction="out"/>
    </method>
    <method name="setValue">
      <arg name="value" type="i" direction="in"/>
    </method>
    <method name="value">
      <arg type="i" direction="out"/>
    </method>
    <method name="setLabel">
      <arg name="label" type="s" direction="in"/>
    </method>
    <method name="showCancelButton">
      <arg name="value" type="b" direction="in"/>
    </method>
    <method name="wasCancelled">
      <arg type="b" direction="out"/>
    </method>
    <method name="setAutoClose">
      <arg name="value" type="b" direction="in"/>
    </method>
    <method name="autoClose">
      <arg type="b" direction="out"/>
    </method>
    <method name="close"/>
  </interface>
</node>
