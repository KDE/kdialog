dbusRef=`./kdialog --geometry 300x200+100-100 --progressbar "Initialising" 6`
dbus $dbusRef setProgress 1
dbus $dbusRef setLabel "Thinking really hard"
sleep 2
dbus $dbusRef setProgress 2
sleep 2
dbus $dbusRef setLabel "Thinking some more"
dbus $dbusRef setProgress 3
sleep 2
dbus $dbusRef setProgress 4
sleep 2
dbus $dbusRef setLabel "Finishing up"
dbus $dbusRef setProgress 5
sleep 1
dbus $dbusRef setProgress 6
sleep 1
dbus $dbusRef close
