<!-- The File Menu  -->
<sect2>
<title><menuchoice>
<shortcut>
<keycombo><keycap>Alt</keycap><keycap>f</keycap></keycombo>
</shortcut>
<guimenu>File</guimenu>
</menuchoice>
</title>

<para>
<variablelist>
<varlistentry>
<term><menuchoice>
<!-- <shortcut><keycap>n</keycap></shortcut> -->
<guimenu>File</guimenu>
<guimenuitem>New</guimenuitem>
</menuchoice></term>
<listitem><para><action>Create a new Text or HTML file, Directory, File System 
Device, Application or URL</action></para></listitem>
